{
	"name": "@krunkscript/highlighter",
	"displayName": "KrunkScript Highlighter",
	"description": "KrunkScript syntax highlighter",
	"icon": "img/icon.png",
	"version": "1.0.6",
	"publisher": "Yendis",
	"author": "KPal",
	"license": "Proprietary",
	"repository": {
		"type": "git",
		"url": "https://github.com/krunkscript/krunkscript.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "krnk",
				"aliases": [
					"KrunkScript",
					"krnk"
				],
				"icon": {
					"dark": "./img/file_type_ks.svg",
					"light": "./img/file_type_ks.svg"
				},
				"extensions": [
					".krnk"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "krnk",
				"scopeName": "source.krnk",
				"path": "./syntaxes/krnk.tmLanguage.json"
			}
		]
	},
	"main": "./src/index.js",
	"activationEvents": [],
	"scripts": {
		"build": "vsce package",
		"release": "vsce publish",
		"lint": "eslint .",
		"prettier": "prettier --write ."
	},
	"devDependencies": {
		"eslint": "^8.20.0",
		"prettier": "^2.7.1"
	}
}
